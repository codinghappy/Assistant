            <TextView
                android:id="@+id/Set_Alarm_LOUYE_Title_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/Set_Alarm_LOUYE_Title"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="12dp"
                android:layout_toRightOf="@+id/Set_Alarm_LOUYE_Title"
