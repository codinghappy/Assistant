            </CheckBox>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/params_set_alert_cell"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/params_set_alert_louye"
